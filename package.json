{
  "name": "StandingTogether",
  "license": "GPL-3.0",
  "scripts": {
    "client": "cd client && yarn start",
    "server": "nodemon --ignore 'client/' server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "start": "concurrently --kill-others-on-fail \"node server -p $PORT\" \"yarn client -p $PORT\" "
  },
  "dependencies": {
    "body-parser": "latest",
    "concurrently": "^4.1.1",
    "cookie-parser": "^1.4.3",
    "dotenv": "^8.0.0",
    "express": "^4.17.3",
    "file-loader": "^1.1.11",
    "file-saver": "^2.0.2",
    "js-cookie": "^2.2.0",
    "multer": "^1.3.1",
    "mysql": "^2.17.1",
    "node-fetch": "latest",
    "node-schedule": "^1.3.2",
    "nodemailer": "^4.6.7",
    "nodemon": "^1.19.1",
    "request": "^2.88.0"
  }
}
